{
  "name": "global-ban-list",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www",
    "test": "./node_modules/.bin/mocha --exit",
    "test:cover": "nyc --report-dir ./coverage --reporter=html --reporter=text-summary node_modules/mocha/bin/_mocha --exit",
    "test:coveralls": "nyc npm run test:cover  && nyc report --reporter=text-lcov | coveralls",
    "lint": "./node_modules/.bin/eslint routes/ models/ helpers/ lib/ app.js public/js",
    "lint:fix": "npm run lint -- --fix",
    "db:seed": "./node_modules/.bin/sequelize db:seed:all",
    "db:migrate": "./node_modules/.bin/sequelize db:migrate",
    "db:init": "npm run db:migrate && npm run db:seed",
    "docs:api": "./node_modules/.bin/apidoc -i routes/ -o docs/api/"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^6.1.0",
    "express": "~4.17.3",
    "express-session": "^1.16.1",
    "http-errors": "~1.6.2",
    "lodash": "^4.17.11",
    "luxon": "^1.4.5",
    "morgan": "~1.9.0",
    "mysql2": "^1.6.1",
    "passport": "^0.4.0",
    "passport-steam": "^1.0.10",
    "pug": "^2.0.3",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5",
    "sequelize": "^4.41.0",
    "validator": "^10.8.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "apidoc": "^0.17.7",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "coveralls": "^3.0.3",
    "eslint": "^5.8.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "faker": "^4.1.0",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^13.3.0",
    "sequelize-auto-migrations": "^1.0.3",
    "sequelize-cli": "^5.2.0"
  }
}
